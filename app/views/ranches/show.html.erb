<div class="ranches_show">
  <div class="date">
    <%= date_display(@ranch) %>
    <%= button_to_next_week(@ranch) %>
  </div>

  <div class="notice">
    <%= notice(@ranch) %>
  </div>

  <div class="mares">
    <%= render 'mares' %>
  </div>

  <div class="racers">
    <%= render 'racers/racers' %>

    <div class="links">
      <%= link_to '新規競走馬', new_racer_path(ranch_id: @ranch.id) %>
    </div>
  </div>
</div>
